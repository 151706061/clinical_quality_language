   <statements>
      <def name="InDemographic" context="PATIENT">
         <expression xsi:type="And">
            <operand xsi:type="And">
               <operand xsi:type="GreaterOrEqual">
                  <operand name="Age" xsi:type="FunctionRef"/>
                  <operand valueType="xsd:int" value="16" xsi:type="Literal"/>
               </operand>
               <operand xsi:type="Less">
                  <operand name="Age" xsi:type="FunctionRef"/>
                  <operand valueType="xsd:int" value="24" xsi:type="Literal"/>
               </operand>
            </operand>
            <operand xsi:type="In">
               <operand/>
               <operand name="Female Administrative Sex" xsi:type="ValueSetRef"/>
            </operand>
         </expression>
      </def>
      <def name="SexuallyActive" context="PATIENT">
         <expression xsi:type="Or">
            <operand xsi:type="Or">
               <operand xsi:type="Or">
                  <operand xsi:type="Or">
                     <operand xsi:type="Or">
                        <operand xsi:type="Or">
                           <operand xsi:type="Or">
                              <operand xsi:type="Or">
                                 <operand xsi:type="Or">
                                    <operand xsi:type="Or">
                                       <operand xsi:type="Or">
                                          <operand xsi:type="IsNotEmpty">
                                             <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                                <codes name="Other Female Reproductive Conditions" xsi:type="ValueSetRef"/>
                                             </operand>
                                          </operand>
                                          <operand xsi:type="IsNotEmpty">
                                             <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                                <codes name="Genital Herpes" xsi:type="ValueSetRef"/>
                                             </operand>
                                          </operand>
                                       </operand>
                                       <operand xsi:type="IsNotEmpty">
                                          <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                             <codes name="Genococcal Infections and Venereal Diseases" xsi:type="ValueSetRef"/>
                                          </operand>
                                       </operand>
                                    </operand>
                                    <operand xsi:type="IsNotEmpty">
                                       <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                          <codes name="Inflammatory Diseases of Female Reproductive Organs" xsi:type="ValueSetRef"/>
                                       </operand>
                                    </operand>
                                 </operand>
                                 <operand xsi:type="IsNotEmpty">
                                    <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                       <codes name="Chlamydia" xsi:type="ValueSetRef"/>
                                    </operand>
                                 </operand>
                              </operand>
                              <operand xsi:type="IsNotEmpty">
                                 <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                    <codes name="HIV" xsi:type="ValueSetRef"/>
                                 </operand>
                              </operand>
                           </operand>
                           <operand xsi:type="IsNotEmpty">
                              <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                                 <codes name="Syphilis" xsi:type="ValueSetRef"/>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="IsNotEmpty">
                           <operand dataType="quick:ConditionOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                              <codes name="Complications of Pregnancy, Childbirth and the Puerperium" xsi:type="ValueSetRef"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="IsNotEmpty">
                        <operand dataType="xsd:LaboratoryTestOrderOccurrence" xsi:type="ClinicalRequest">
                           <codes name="Pregnancy Test" xsi:type="ValueSetRef"/>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="IsNotEmpty">
                     <operand dataType="xsd:LaboratoryTestOrderOccurrence" xsi:type="ClinicalRequest">
                        <codes name="Pap Test" xsi:type="ValueSetRef"/>
                     </operand>
                  </operand>
               </operand>
               <operand xsi:type="IsNotEmpty">
                  <operand dataType="xsd:LaboratoryTestOrderOccurrence" xsi:type="ClinicalRequest">
                     <codes name="Lab Tests During Pregnancy" xsi:type="ValueSetRef"/>
                  </operand>
               </operand>
            </operand>
            <operand xsi:type="IsNotEmpty">
               <operand dataType="xsd:LaboratoryTestOrderOccurrence" xsi:type="ClinicalRequest">
                  <codes name="Lab Tests for Sexually Transmitted Infections" xsi:type="ValueSetRef"/>
               </operand>
            </operand>
         </expression>
      </def>
      <def name="NoScreening" context="PATIENT">
         <expression xsi:type="And">
            <operand xsi:type="And">
               <operand xsi:type="Not">
                  <operand xsi:type="IsNotEmpty">
                     <operand xsi:type="Query">
                        <source alias="R">
                           <expression dataType="quick:SimpleObservationOccurrence" codeProperty="code" dateProperty="observedAtTime" xsi:type="ClinicalRequest">
                              <codes name="Chlamydia Screening" xsi:type="ValueSetRef"/>
                              <dateRange beginOpen="false" endOpen="false" xsi:type="Interval">
                                 <begin xsi:type="Subtract">
                                    <operand name="Today" xsi:type="FunctionRef"/>
                                    <operand value="1" unit="years" xsi:type="Quantity"/>
                                 </begin>
                                 <end name="Today" xsi:type="FunctionRef"/>
                              </dateRange>
                           </expression>
                        </source>
                        <where xsi:type="Not">
                           <operand xsi:type="IsNull">
                              <operand xsi:type="Null"/>
                           </operand>
                        </where>
                     </operand>
                  </operand>
               </operand>
               <operand xsi:type="Not">
                  <operand xsi:type="IsNotEmpty">
                     <operand dataType="xsd:ProcedurePlanOccurrence" xsi:type="ClinicalRequest">
                        <codes name="Chlamydia Screening" xsi:type="ValueSetRef"/>
                        <dateRange beginOpen="false" endOpen="false" xsi:type="Interval">
                           <begin name="Today" xsi:type="FunctionRef"/>
                           <end xsi:type="Null"/>
                        </dateRange>
                     </operand>
                  </operand>
               </operand>
            </operand>
            <operand xsi:type="Not">
               <operand xsi:type="IsNotEmpty">
                  <operand dataType="quick:SimpleObservationOccurrence" codeProperty="code" xsi:type="ClinicalRequest">
                     <codes name="Reason for not performing Chlamydia Screening" xsi:type="ValueSetRef"/>
                  </operand>
               </operand>
            </operand>
         </expression>
      </def>
      <def name="NeedScreening" context="PATIENT">
         <expression xsi:type="And">
            <operand xsi:type="And">
               <operand name="InDemographic" xsi:type="ExpressionRef"/>
               <operand name="SexuallyActive" xsi:type="ExpressionRef"/>
            </operand>
            <operand name="NoScreening" xsi:type="ExpressionRef"/>
         </expression>
      </def>
