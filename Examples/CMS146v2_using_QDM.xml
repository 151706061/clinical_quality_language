<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="urn:hl7-org:elm:r1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:quick="http://hl7.org/fhir" xmlns:a="urn:hl7-org:cql-annotations:r1">
   <identifier id="CMS146_QDM" version="2"/>
   <schemaIdentifier id="urn:hl7-org:elm" version="r1"/>
   <usings>
      <def localIdentifier="System" uri="urn:hl7-org:elm:r1"/>
      <def localIdentifier="QDM" uri="urn:healthit-gov:qdm:v4_1_2"/>
   </usings>
   <parameters>
      <def name="MeasurementPeriod">
         <default lowClosed="true" highClosed="false" xsi:type="Interval">
            <low name="DateTime" xsi:type="FunctionRef">
               <operand valueType="Integer" value="2013" xsi:type="Literal"/>
               <operand valueType="Integer" value="1" xsi:type="Literal"/>
               <operand valueType="Integer" value="1" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
            </low>
            <high name="DateTime" xsi:type="FunctionRef">
               <operand valueType="Integer" value="2014" xsi:type="Literal"/>
               <operand valueType="Integer" value="1" xsi:type="Literal"/>
               <operand valueType="Integer" value="1" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
               <operand valueType="Integer" value="0" xsi:type="Literal"/>
            </high>
         </default>
      </def>
   </parameters>
   <valueSets>
      <def name="Acute Pharyngitis" id="2.16.840.1.113883.3.464.1003.102.12.1011"/>
      <def name="Acute Tonsillitis" id="2.16.840.1.113883.3.464.1003.102.12.1012"/>
      <def name="Ambulatory/ED Visit" id="2.16.840.1.113883.3.464.1003.101.12.1061"/>
      <def name="Antibiotic Medications" id="2.16.840.1.113883.3.464.1003.196.12.1001"/>
      <def name="Group A Streptococcus Test" id="2.16.840.1.113883.3.464.1003.198.12.1012"/>
   </valueSets>
   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand xmlns:ns0="urn:healthit-gov:qdm:v4_1_2" dataType="ns0:Patient" templateId="Patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="InDemographic" context="Patient">
         <expression xsi:type="And">
            <operand xsi:type="GreaterOrEqual">
               <operand precision="Year" xsi:type="CalculateAgeAt">
                  <operand path="birth datetime" xsi:type="Property">
                     <source name="Patient" xsi:type="ExpressionRef"/>
                  </operand>
                  <operand xsi:type="Start">
                     <operand name="MeasurementPeriod" xsi:type="ParameterRef"/>
                  </operand>
               </operand>
               <operand valueType="Integer" value="2" xsi:type="Literal"/>
            </operand>
            <operand xsi:type="Less">
               <operand precision="Year" xsi:type="CalculateAgeAt">
                  <operand path="birth datetime" xsi:type="Property">
                     <source name="Patient" xsi:type="ExpressionRef"/>
                  </operand>
                  <operand xsi:type="Start">
                     <operand name="MeasurementPeriod" xsi:type="ParameterRef"/>
                  </operand>
               </operand>
               <operand valueType="Integer" value="18" xsi:type="Literal"/>
            </operand>
         </expression>
      </def>
      <def name="Pharyngitis" context="Patient">
         <expression xsi:type="Union">
            <operand xmlns:ns1="urn:healthit-gov:qdm:v4_1_2" dataType="ns1:DiagnosisActive" templateId="DiagnosisActive" codeProperty="code" xsi:type="Retrieve">
               <codes name="Acute Pharyngitis" xsi:type="ValueSetRef"/>
            </operand>
            <operand xmlns:ns2="urn:healthit-gov:qdm:v4_1_2" dataType="ns2:DiagnosisActive" templateId="DiagnosisActive" codeProperty="code" xsi:type="Retrieve">
               <codes name="Acute Tonsillitis" xsi:type="ValueSetRef"/>
            </operand>
         </expression>
      </def>
      <def name="Antibiotics" context="Patient">
         <expression xmlns:ns3="urn:healthit-gov:qdm:v4_1_2" dataType="ns3:MedicationOrder" templateId="MedicationOrder" codeProperty="code" xsi:type="Retrieve">
            <codes name="Antibiotic Medications" xsi:type="ValueSetRef"/>
         </expression>
      </def>
      <def name="TargetEncounters" context="Patient">
         <expression xsi:type="Query">
            <source alias="E">
               <expression xmlns:ns4="urn:healthit-gov:qdm:v4_1_2" dataType="ns4:EncounterPerformed" templateId="EncounterPerformed" codeProperty="code" xsi:type="Retrieve">
                  <codes name="Ambulatory/ED Visit" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <relationship alias="P" xsi:type="With">
               <expression name="Pharyngitis" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="OverlapsAfter">
                  <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                     <low path="start datetime" scope="P" xsi:type="Property"/>
                     <high path="stop datetime" scope="P" xsi:type="Property"/>
                  </operand>
                  <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                     <low path="start datetime" scope="E" xsi:type="Property"/>
                     <high path="stop datetime" scope="E" xsi:type="Property"/>
                  </operand>
               </suchThat>
            </relationship>
            <relationship alias="A" xsi:type="With">
               <expression name="Antibiotics" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="In">
                  <operand precision="Day" xsi:type="DurationBetween">
                     <operand path="signed datetime" scope="A" xsi:type="Property"/>
                     <operand path="start datetime" scope="E" xsi:type="Property"/>
                  </operand>
                  <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                     <low xsi:type="Negate">
                        <operand valueType="Integer" value="3" xsi:type="Literal"/>
                     </low>
                     <high valueType="Integer" value="0" xsi:type="Literal"/>
                  </operand>
               </suchThat>
            </relationship>
            <where xsi:type="IncludedIn">
               <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                  <low path="start datetime" scope="E" xsi:type="Property"/>
                  <high path="stop datetime" scope="E" xsi:type="Property"/>
               </operand>
               <operand name="MeasurementPeriod" xsi:type="ParameterRef"/>
            </where>
         </expression>
      </def>
      <def name="TargetDiagnoses" context="Patient">
         <expression xsi:type="Query">
            <source alias="P">
               <expression name="Pharyngitis" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="E" xsi:type="With">
               <expression name="TargetEncounters" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="OverlapsAfter">
                  <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                     <low path="start datetime" scope="P" xsi:type="Property"/>
                     <high path="stop datetime" scope="P" xsi:type="Property"/>
                  </operand>
                  <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                     <low path="start datetime" scope="E" xsi:type="Property"/>
                     <high path="stop datetime" scope="E" xsi:type="Property"/>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="HasPriorAntibiotics" context="Patient">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="A">
                  <expression name="Antibiotics" xsi:type="ExpressionRef"/>
               </source>
               <relationship alias="D" xsi:type="With">
                  <expression name="TargetDiagnoses" xsi:type="ExpressionRef"/>
                  <suchThat xsi:type="In">
                     <operand precision="Day" xsi:type="DurationBetween">
                        <operand path="signed datetime" scope="A" xsi:type="Property"/>
                        <operand path="start datetime" scope="D" xsi:type="Property"/>
                     </operand>
                     <operand lowClosed="false" highClosed="true" xsi:type="Interval">
                        <low valueType="Integer" value="0" xsi:type="Literal"/>
                        <high valueType="Integer" value="30" xsi:type="Literal"/>
                     </operand>
                  </suchThat>
               </relationship>
            </operand>
         </expression>
      </def>
      <def name="HasTargetEncounter" context="Patient">
         <expression xsi:type="Exists">
            <operand name="TargetEncounters" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="InInitialPopulation" context="Patient">
         <expression xsi:type="And">
            <operand name="InDemographic" xsi:type="ExpressionRef"/>
            <operand name="HasTargetEncounter" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="InDenominator" context="Patient">
         <expression valueType="Boolean" value="true" xsi:type="Literal"/>
      </def>
      <def name="InDenominatorExclusions" context="Patient">
         <expression name="HasPriorAntibiotics" xsi:type="ExpressionRef"/>
      </def>
      <def name="InNumerator" context="Patient">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="R">
                  <expression xmlns:ns5="urn:healthit-gov:qdm:v4_1_2" dataType="ns5:LaboratoryTestPerformed" templateId="LaboratoryTestPerformed" codeProperty="code" xsi:type="Retrieve">
                     <codes name="Group A Streptococcus Test" xsi:type="ValueSetRef"/>
                  </expression>
               </source>
               <where xsi:type="And">
                  <operand xsi:type="In">
                     <operand path="start datetime" scope="R" xsi:type="Property"/>
                     <operand name="MeasurementPeriod" xsi:type="ParameterRef"/>
                  </operand>
                  <operand xsi:type="Not">
                     <operand xsi:type="IsNull">
                        <operand path="result" scope="R" xsi:type="Property"/>
                     </operand>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
   </statements>
</library>


